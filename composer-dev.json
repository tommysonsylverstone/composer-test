{
    "repositories":[
        {
            "type": "path",
            "url": "../../../assets/admin/node_modules",
            "options": {
                "symlink": true
            }
        }
    ],
    "require": {
        "tommysonsylverstone/composer-test": "dev-main"
    }
    "extra": {
        "merge-plugin": {
            "include": [
                "src/*/composer.json"
            ],
            "recurse": true,
            "replace": false,
            "ignore-duplicates": true,
            "merge-dev": true,
            "merge-extra": false,
            "merge-extra-deep": false,
            "merge-scripts": true
        }
    }
}

